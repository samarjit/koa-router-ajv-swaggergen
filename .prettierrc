{
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "printWidth": 200,
        "tabWidth": 2,
        "useTabs": false,
        "singleQuote": true,
        "bracketSpacing": true,
        "explicitTypes": "always",
        "jsxSingleQuote": false,
        "trailingComma": "es5",
        "semi": true
      }
    }
  ]
}
